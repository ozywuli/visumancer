-index:15}.nav .nav__links--pages{margin-top:20px}.nav .nav__links--pages a{position:relative;display:block;padding:5px 10px}.nav .nav__links--pages a:before,.overlay{position:absolute;top:0;left:0;height:100%}.nav .nav__links--pages a:before{content:"";display:block;width:0;transition:all 150ms ease}.meta-actions li,.notes .action{display:inline-block}.nav .nav__links--pages a:hover{padding-left:13px}.nav .nav__links--pages a:hover:before{width:3px}.content,.overlay,.post img{width:100%}.container{background:#f1f1f1;padding:15px 0 15px 15px;overflow-x:hidden}.permalink-page .container{padding:100px 15px 60px}.permalink-page .content{margin:0 auto;max-width:600px}.overlay{right:0;bottom:0;background:rgba(0,0,0,.5)}.notes,.permalink-pagination a,.post-container{box-shadow:0 1px 2px 0 rgba(0,0,0,.1);background:#FFF}.post{color:#333}.post figure{margin:0}.body-text,.permalink-page .post,.post img,.post p{margin-bottom:10px}.meta-actions li,.permalink-pagination .next{margin-left:10px}.post img{height:auto}.post a{color:#888}.post a:hover{color:#56AAC3}.post-container{padding:10px 10px 20px}.post__meta{margin-top:20px;font-size:1.2rem;line-height:1.5rem}.grid-item,.grid-sizer{width:50%}.grid-item{float:left;padding:0 15px 15px 0}.post__meta-separator{color:#888}.meta-actions{float:right;font-size:0}.permalink-pagination{width:100%;margin-bottom:20px;font-size:0;text-transform:uppercase}.permalink-pagination a{display:inline-block;width:calc(50% - 5px);font-size:2rem;text-align:center;padding:10px 0}.notes{padding:10px}.notes .note{margin-bottom:10px}.notes img{width:24px;height:24px;margin:0}.notes .action{margin-left:10px;line-height:24px;vertical-align:top}.more_notes_link{display:block;margin-top:20px}@media all and (min-width:600px){.masthead{height:400px}}@media all and (min-width:900px){.content{margin:0 auto;max-width:1400px}.grid-item,.grid-sizer{width:33.33%}}@media all and (min-width:1200px){.grid-item,.grid-sizer{width:25%}.yellowfin{width:50%}}
  </style>
  <style type="text/css">
    /*Tumblr Global Appearance */
    body {
      background:{BackgroundColor};
      font-family: {font:Body}
    }
    .topbar__title, .masthead__title {
      font-family: {TitleFont};
    }
    h1 > a {color:{TitleColor};}
    a {color:{AccentColor}}
    a:hover {
      color: {color:Link Hover}
    }
    
    .masthead__bg {
      background-image: url('{HeaderImage}');
    }
    
    .menu-toggle:hover svg {
    
      fill: {color:Link Hover};
    
    }
    
    .nav__links--pages a:before {
      background: {color:Link Hover};
    }
    /*Custom CSS*/
    {CustomCSS}
  </style>

</head>




<body class="{block:PermalinkPage}permalink-page{/block:PermalinkPage}">


<!-- #topbar -->
<div role="topbar" class="topbar">

  <a href="#" class="menu-toggle">
    <span class="hidden">Menu Toggle</span>
      <svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px"
         width="25px" height="25px" viewBox="159.669 159.669 25 25" enable-background="new 159.669 159.669 25 25" xml:space="preserve">
      <g>
        <g>
          <g>
            <rect x="159.67" y="163.014" width="25" height="2.143"/>
          </g>
          <g>
            <rect x="159.67" y="171.032" width="25" height="2.143"/>
          </g>
          <g>
            <rect x="159.67" y="179.181" width="25" height="2.145"/>
          </g>
        </g>
      </g>
      </svg>
      
  </a>

  <h1 class="topbar__title"><a href="/">{Title}</a></h1>

  <form class="search" action="/search" method="get">
    <input role="textbox" type="search" placeholder="{lang:Search}" name="q" value="{SearchQuery}">
    <input role="button" type="submit" value="{lang:Search}">
  </form>



</div>
<!-- /topbar -->

<!-- #nav -->

<nav role="navigation" class="nav">
  <ul class="nav__links--pages">
    {block:HasPages}{block:Pages}<li><a href="{URL}">{Label}</a></li>{/block:Pages}{/block:HasPages}
    <li><a href="/archive">{lang:Archive}</a></li>
    {block:AskEnabled}<li><a href="/ask">{AskLabel}</a></li>{/block:AskEnabled}
  </ul>
</nav>
<!-- /nav -->

{block:IndexPage}
  <!-- #header -->
  <header role="banner" class="masthead">
  
    {block:ShowDescription}
      {block:Description}
      <div class="description">{Description}</div>
      {/block:Description}
    {/block:ShowDescription}
    
    {block:ShowHeaderImage}
      <div class="masthead__bg"></div>
    {/block:ShowHeaderImage}
  
      <div class="masthead__intro">
        {block:ShowAvatar}
          <img src="{PortraitURL-128}" class="masthead__avatar">
        {/block:ShowAvatar}
        {block:ShowTitle}
          <h1 class="masthead__title"><a href="/">{Title}</a></h1>
        {/block:ShowTitle}
      </div>
  
  
  
      <div class="overlay"></div>
  
  
  
  </header>
  
  
  
  
  <!-- /header -->
{/block:IndexPage}

<div class="container">

  <!-- #main -->
  <main role="main" class="content clearfix {block:IndexPage}js-content{/block:IndexPage}">

    {block:SearchPage}
    <div class="result">
      <p>{lang:Found SearchResultCount results for SearchQuery 2}</p>
      {block:NoSearchResults}<p>{lang:No results for SearchQuery 2}</p>{/block:NoSearchResults}
    </div>
    {/block:SearchPage}

    {block:TagPage}<div class="result"><p>{lang:TagResultCount posts tagged Tag 3}</p></div>{/block:TagPage}

    <!-- #post types -->
    {block:Posts}
    <article class="post {PostType} {TagsAsClasses} {block:IndexPage}js-post grid-item grid-sizer{/block:IndexPage}" id="{PostID}">

      <div class="post-container">
      {block:Text}
        {block:Body}
        <div class="body-text">
          {Body}
        </div>
        {/block:Body}
        {block:Title}<h2><a href="{Permalink}">{Title}</a></h2>{/block:Title}
      {/block:Text}
      {block:Quote}
        <blockquote class="words {Length}">&#8220;{Quote}&#8221;</blockquote>
        {block:Source}<p class="source">&mdash; {Source}</p>{/block:Source}
      {/block:Quote}
      {block:Link}
        <h2><a href="{URL}" {Target}>{Name}</a></h2>
        {block:Description}<div class="caption">{Description}</div>{/block:Description}
      {/block:Link}
      {block:Audio}
        {block:AlbumArt}<img src="{AlbumArtURL}" alt="">{/block:AlbumArt}
        <div class="audio-player">
          {block:AudioEmbed}
            {AudioEmbed color="white"}
          {/block:AudioEmbed}
          {block:AudioPlayer}{AudioPlayerBlack}{/block:AudioPlayer}
        </div>
        {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
      {/block:Audio}
      {block:Photo}
        <figure class="{block:HighRes}high-res{/block:HighRes}{block:Caption} with-caption{/block:Caption}" data-photo-width="{PhotoWidth-HighRes}">
          {LinkOpenTag}<img src="{PhotoURL-HighRes}" alt="{PhotoAlt}" width="{PhotoWidth-HighRes}" height="{PhotoHeight-HighRes}">{LinkCloseTag}
          {block:Caption}<figcaption class="caption">{Caption}</figcaption>{/block:Caption}
        </figure>
      {/block:Photo}
      {block:Photoset}
        <figure{block:Caption} class="with-caption"{/block:Caption}>
          {block:Photos}
          <img src="{PhotoURL-HighRes}" alt="{PhotoAlt}"/>
          {block:Caption}<div class="caption">{Caption}</div>{/block:Caption}
          {/block:Photos}
          {block:Caption}<figcaption class="caption">{Caption}</figcaption>{/block:Caption}
        </figure>
      {/block:Photoset}
      {block:Panorama}
        <figure{block:Caption} class="with-caption"{/block:Caption}>
          {LinkOpenTag}<img src="{PhotoURL-Panorama}" width="{PhotoWidth-Panorama}" height="{PhotoHeight-Panorama}" alt="{PhotoAlt}" />{LinkCloseTag}
          {block:Caption}<figcaption class="caption">{Caption}</figcaption>{/block:Caption}
        </figure>
      {/block:Panorama}
      {block:Chat}
        {block:Title}<h2>{Title}</h2>{/block:Title}
        <ul class="conversation">
          {block:Lines}
          <li class="line {Alt}">
            {block:Label}<span class="person">{Label}</span>{/block:Label}
            <span class="person-said">{Line}</span>
          </li>
          {/block:Lines}
        </ul>
      {/block:Chat}
      {block:Answer}
        <section class="question">
          <img src="{AskerPortraitURL-48}">
          <div class="asker">{Asker} asked:</div>
          <div class="asker-question">
            {Question}
          </div>
        </section>
        {block:Answerer}
          <section class="answerer">
            <img src="{AnswererPortraitURL-48}">
            <p>{Answerer} answered:</p>
            <div class="answerer-answer">
              {Answer}
            </div>
          </section>
        {/block:Answerer}
        <div class="replies">{Replies}</div>
      {/block:Answer}


      <div class="post__meta">
        {block:Date}
          {block:NewDayDate}<a href="{Permalink}">{DayOfMonth} {ShortMonth} {Year}</a>{/block:NewDayDate}
          {block:SameDayDate}<a href="{Permalink}">{DayOfMonth} {ShortMonth} {Year}</a>{/block:SameDayDate}
        {/block:Date}

        <span class="post__meta-separator">|</span>

        <a href="{Permalink}" class="note-count">{NoteCountWithLabel}</a>

        {block:IndexPage}
        <div class="meta-actions">
          <ul>
            <li>{LikeButton color="grey" size="15"}</li>
            <li>{ReblogButton color="grey" size="15"}</li>
            <li><a href="{Permalink}" class="permalink"><span class="hidden">{lang:Permalink}</span></a></li>
          </ul>
        </div>
        {/block:IndexPage}
        </div>
      </div>

    </article>
    {/block:Posts}
    <!-- #post types -->


    <!-- #permalink pagination -->
    {block:PermalinkPagination}
    <div class="permalink-pagination">
      {block:PreviousPost}<a class="prev"href="{PreviousPost}">{lang:Prev}</a>{/block:PreviousPost}
      {block:NextPost}<a class="next" href="{NextPost}">{lang:Next}</a>{/block:NextPost}
    </div>
    {/block:PermalinkPagination}
    
    <!-- /permalink pagination -->

      {block:PostNotes}
      {PostNotes-64}
      {/block:PostNotes}


    <!-- #index pagination -->
    {block:Pagination}
    <div class="pagination">
      {block:PreviousPage}<a class="pagination__prev" href="{PreviousPage}">{lang:Previous}</a>{/block:PreviousPage}
    
      {block:JumpPagination length="5"}
        {block:CurrentPage}<span class="current-page">{PageNumber}</span>{/block:CurrentPage}
        {block:JumpPage}<a class="jump-page" href="{URL}">{PageNumber}</a>{/block:JumpPage}
      {/block:JumpPagination}
    
      {block:NextPage}<a class="pagination__next" href="{NextPage}">{lang:Next}</a>{/block:NextPage}
    </div>
    {/block:Pagination}
    <!-- /index pagination -->



  </main><!-- /main -->


</div>

  <!-- #footer -->
  <footer class="footer">
    <small>{block:ShowTitle}{Title} {/block:ShowTitle}&copy; {CopyrightYears}</small>
  </footer>
  
  <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.0.0-alpha1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-infinitescroll/2.1.0/jquery.infinitescroll.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/masonry/3.3.2/masonry.pkgd.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.1.8/imagesloaded.pkgd.min.js"></script>
   -->
  
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-infinitescroll/2.0b2.120519/jquery.infinitescroll.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/masonry/3.1.2/masonry.pkgd.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.imagesloaded/3.0.4/jquery.imagesloaded.min.js"></script>
  
  <!-- #custom js -->
  <script>
  (function() {
  
    // Main content container
    var $container = $('.js-content');
  
    // Masonry + ImagesLoaded
    $container.imagesLoaded(function() {
      $container.masonry({
        // selector for entry content
        itemSelector: '.js-post',
        columnWidth: '.grid-sizer',
        percentPosition: true
      });
    });
  
    // Infinite Scroll
    $container.infinitescroll({
  
      // selector for the paged navigation (it will be hidden)
      navSelector  : ".pagination",
      // selector for the NEXT link (to page 2)
      nextSelector : ".pagination__next",
      // selector for all items you'll retrieve
      itemSelector : ".js-post",
  
      // finished message
      loading: {
        finishedMsg: 'No more pages to load.'
        }
      },
  
      // Trigger Masonry as a callback
      function( newElements ) {
        // hide new items while they are loading
        var $newElems = $( newElements ).css({ opacity: 0 });
        // ensure that images load before adding to masonry layout
        $newElems.imagesLoaded(function(){
          // show elems now they're ready
          $newElems.animate({ opacity: 1 });
          $container.masonry( 'appended', $newElems, true );
        });
  
  
        var $newElemsIDs = $newElems.map(function() {
            console.log(this);
            console.log(this.id);
            return this.id
        }).get();
  
        Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
      }
  
    );
    
    /**
     * OPTIONAL!
     * Load new pages by clicking a link
     */
  
    // Pause Infinite Scroll
  /*  $(window).unbind('.infscr');
  
    // Resume Infinite Scroll
    $('.pagintion__prev').click(function(){
      $container.infinitescroll('retrieve');
      return false;
    });*/
  
  })();
  </script>
  <!-- /custom -->
  
  </body>
  </html>
  <!-- /footer -->

